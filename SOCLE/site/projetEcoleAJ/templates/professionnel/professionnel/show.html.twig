{% extends 'base.html.twig' %}

{% block title %}Matiere
{% endblock %}

{% block body %}

	<div class="container">

		<h5>Professionnel</h5>
		<hr style="width:100%; height:5px; background-color:#1B4F72 ">

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ professionnel.id }}</td>
				</tr>
				<tr>
					<th>Prenom NOMM</th>
					<td>{{ professionnel.prenom }} {{ professionnel.nom }}</td>
				</tr>
				<tr>
					<th>telephone</th>
					<td>{{ professionnel.telephone }}</td>
				</tr>
				<tr>
					<th>E-mail</th>
					<td>{{ professionnel.email }}</td>
				</tr>
				<tr>
					<th>Entreprise</th>
					<td>{{ professionnel.entreprise.libelle }}</td>
				</tr>
				<tr>
					<th>profession</th>
					<td>{{ professionnel.profession }}</td>
				</tr>
			</tr>
			</tbody>
		</table>

		<a href="{{ path('professionnel_index') }}" class="btn btn-secondary">Liste de sieges</a>

		<a href="{{ path('professionnel_edition', {'id': professionnel.id}) }}" class="btn btn-secondary">Modifier</a>

		{{ include('professionnel/professionnel/_delete_form.html.twig') }}
	</div>
	<br>
	<br>
	<br>
	<br>
	<br>
	<br>

{% endblock %}
